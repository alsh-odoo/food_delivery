<?xml version="1.0" encoding="UTF-8"?>

<odoo>
    <record id="food_delivery_action" model="ir.actions.act_window">
        <field name="name">Order Food</field>
        <field name="res_model">food.delivery</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record id ="restaurant_name_action" model="ir.actions.act_window">
        <field name="name">Restaurant Name</field>
        <field name="res_model">restaurant.name</field>
        <field name = "view_mode">tree,form</field>
    </record>

    <menuitem id="food_delivery_menu_root" name="Food Delivery">
        <menuitem id="order_food" action="food_delivery_action"  name="Order Food"></menuitem>
        <!-- <menuitem id="non_vegitarian_menu"  action="food_delivery_action" name="Non Vegitarian"></menuitem> -->
        <menuitem id="restaurant_name_menu" action="restaurant_name_action" name="Restaurants"></menuitem>
    </menuitem>
</odoo>